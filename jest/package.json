{
  "name": "@growflow/jest",
  "description": "Shareable Jest configuration used in GrowFlow projects",
  "main": "lib/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "version": "5.5.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/GrowflowTeam/javascript.git"
  },
  "author": "GrowFlow",
  "license": "MIT",
  "files": [ "lib", "dist", "README.md" ],
  "keywords": [
    "jest",
    "config",
    "growflow"
  ],
  "scripts": {
    "build": "rimraf lib dist && tsc -p tsconfig.output.json && babel src -d lib --root-mode upward --extensions \".ts\" --source-maps",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@types/jest": "^26.0.20",
    "deepmerge": "^4.2.2",
    "jest": "^26.6.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-ts-auto-mock": "^1.0.12",
    "node-fetch": "^2.6.1",
    "ts-auto-mock": "^3.1.0",
    "ts-jest": "^26.4.4",
    "ttypescript": "^1.5.12"
  },
  "peerDependencies": {
    "jest": "26.x"
  },
  "devDependencies": {
    "rimraf": "^3.0.2"
  }
}
