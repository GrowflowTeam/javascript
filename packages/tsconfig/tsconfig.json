{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "preserve",
    "downlevelIteration": true,
    "noEmit": true,
    "declaration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,

    "strict": true,
    "noImplicitAny": true
  }
}
